<h1>Display your Pirate here</h1>
<div class="pirates">
  <h3>Name: <%= @pirate.name %></h3><br>
  <h4>Weight: <%= @pirate.weight %></h4>
  <h5>Height: <%= @pirate.height %></h5>
</div><br>

<h2>Display your first ship here</h2>
<h1>Courses</h1>
<% @courses.each do |course| %>
  <div class="course">
    <p>Name: <%= course.name %></p><br>
    <p>Topic: <%= course.topic %></p><br>
  </div><br>
<% end %>

<h2>Display your second ship here</h2>
